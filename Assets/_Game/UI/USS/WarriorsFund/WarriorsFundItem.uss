    color: rgba(255, 255, 255, 1);
}


.wf-free-reward-container.claimed .reward-outer-bg {
    opacity: 100%;
    background-color: rgb(94, 119, 195);
    border-width: 3px;
    border-color: black;
    border-radius: 22px;
}

.wf-free-reward-container.claimed .reward-inner-bg {
    background-color: rgb(20, 34, 78);
    border-width: 3px;
    border-color: black;
}

.wf-free-reward-container.claimed .lock-icon {
    visibility: hidden;
}

.wf-free-reward-container.claimed .checkmark-icon {
    visibility: visible;
    -unity-background-image-tint-color: rgb(90, 200, 0);
}

.wf-free-reward-container.claimed .reward-icon {
    -unity-background-image-tint-color: rgba(255, 255, 255, 0.5);
}

.wf-free-reward-container.claimed .amount-label {
    -unity-text-outline-color: rgba(0, 0, 0, 0.5);

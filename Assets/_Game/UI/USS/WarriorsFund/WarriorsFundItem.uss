.wf-control-point {
    background-color: rgb(20, 34, 78);
    border-width: 3px;
    border-color: black;
}

.wf-control-point.active {
    background-color: rgb(237, 195, 67);
    border-width: 3px;
    border-color: black;
}

.wf-free-reward-container {
}


.wf-free-reward-container .reward-outer-bg {
    opacity: 100%;
    background-color: rgb(174, 194, 255);
    border-width: 3px;     
    border-color: black;
    border-radius: 22px;
}

.wf-free-reward-container .reward-inner-bg {
    background-color: rgb(94, 119, 195);
}

.wf-free-reward-container .lock-icon {
    visibility: hidden;
}

.wf-free-reward-container .checkmark-icon {
    visibility: hidden;
    -unity-background-image-tint-color: rgb(90, 200, 0);
}

.wf-free-reward-container .reward-icon {
    -unity-background-image-tint-color: rgba(255, 255, 255, 1);
}

.wf-free-reward-container .amount-label {
    -unity-text-outline-color: rgba(0, 0, 0, 1);
    color: rgba(255, 255, 255, 1);
}


.wf-free-reward-container.active .reward-outer-bg {
    opacity: 100%;
    background-color: rgb(187, 243, 141);
    border-width: 3px;   
    border-color: black;
    border-radius: 22px;
}

.wf-free-reward-container.active .reward-inner-bg {
    background-color: rgb(90, 200, 0);
    border-width: 3px;   
    border-color: black;   
}

.wf-free-reward-container.active .lock-icon {
    visibility: hidden;
}

.wf-free-reward-container.active .checkmark-icon {
    visibility: hidden;
    -unity-background-image-tint-color: rgb(90, 200, 0);
}

.wf-free-reward-container.active .reward-icon {
    -unity-background-image-tint-color: rgba(255, 255, 255, 1);
}

.wf-free-reward-container.active .amount-label {

.unity-vertical-progress-bar__container {
    max-height: 100%;
    height: 100%;
    max-width: 1000px;
    width: 30px;
}

.unity-vertical-progress-bar__background {
    background-color: #2B3C3A;
    border-color: black;
    border-width: 0;
    border-radius: 17px;
    height: 100%;
}

.unity-vertical-progress-bar__progress {
    background-color: #FF4D47;
    border-color: black;
    border-width: 3px;
    border-radius: 17px;
    margin: -3px;
}

